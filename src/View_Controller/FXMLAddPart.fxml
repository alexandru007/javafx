<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="AddPartWindow" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.FXMLAddPartController">
   <children>
      <Label layoutX="28.0" layoutY="33.0" prefHeight="17.0" prefWidth="104.0" text="Add Part">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="TextFieldID" editable="false" layoutX="250.0" layoutY="77.0" prefHeight="33.0" prefWidth="154.0" promptText="Auto Gen - Disabled" />
      <TextField fx:id="maxTextField" layoutX="178.0" layoutY="251.0" prefHeight="33.0" prefWidth="70.0" promptText="max" />
      <TextField fx:id="nameTextField" layoutX="250.0" layoutY="123.0" prefHeight="33.0" prefWidth="154.0" promptText="Part name" />
      <TextField fx:id="inventoryTextField" layoutX="250.0" layoutY="167.0" prefHeight="33.0" prefWidth="154.0" promptText="Inv" />
      <Label layoutX="112.0" layoutY="85.0" prefHeight="17.0" prefWidth="104.0" text="ID">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="112.0" layoutY="131.0" prefHeight="17.0" prefWidth="104.0" text="Name">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="111.0" layoutY="175.0" prefHeight="17.0" prefWidth="104.0" text="Inv">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="114.0" layoutY="259.0" prefHeight="17.0" prefWidth="37.0" text="Max">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="282.0" layoutY="259.0" prefHeight="17.0" prefWidth="37.0" text="Min">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="minTextField" layoutX="334.0" layoutY="251.0" prefHeight="33.0" prefWidth="70.0" promptText="min" />
      <TextField fx:id="companyNameTextField" layoutX="250.0" layoutY="296.0" prefHeight="33.0" prefWidth="154.0" promptText="comp name" />
      <Label fx:id="companyNameLabel" layoutX="115.0" layoutY="304.0" prefHeight="17.0" prefWidth="98.0" text="Company Name">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="ButtonSave" layoutX="377.0" layoutY="354.0" mnemonicParsing="false" onAction="#LoadMainSceneWithUpdatedPart" text="Save" />
      <Button fx:id="ButtonCancel" layoutX="453.0" layoutY="354.0" mnemonicParsing="false" onAction="#LoadMainScene" text="Cancel" />
      <TextField fx:id="priceTextField" layoutX="250.0" layoutY="210.0" prefHeight="33.0" prefWidth="154.0" promptText="price" />
      <Label layoutX="112.0" layoutY="218.0" prefHeight="17.0" prefWidth="104.0" text="Price/Cost">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <RadioButton fx:id="inHouseCheckBox" layoutX="190.0" layoutY="14.0" mnemonicParsing="false" onAction="#updateWindowBasedOnCheckBox" text="In-House" />
      <RadioButton fx:id="outSourcedCheckBox" layoutX="354.0" layoutY="14.0" mnemonicParsing="false" onAction="#updateWindowBasedOnCheckBox" text="Outsourced" />
   </children>
</AnchorPane>
